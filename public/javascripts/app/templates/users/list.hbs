<h1>List of users</h1>
<table class="table table-striped">
    <tr>
        <th>#</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Login</th>
    </tr>
    {{#each this}}
    <tr>
        <td>1</td>
        <td>{{firstName}}</td>
        <td>{{lastName}}</td>
        <td>{{login}}</td>
    </tr>
    {{/each}}
</table>